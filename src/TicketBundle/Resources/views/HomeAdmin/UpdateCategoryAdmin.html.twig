{% extends "base_back.html.twig" %}

{% block content_event %}
    <h1>Update this Ticket category</h1>

    {{ form_start(form) }}
    Type of the category : {{ form_widget(form.typeR) }}</br>
    Content of the category: {{ form_widget(form.content) }}</br>

    <button type="submit" class="btn btn-primary">Update</button>
    {{ form_end(form) }}
{% endblock %}
